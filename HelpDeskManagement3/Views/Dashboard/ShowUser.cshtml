@model IEnumerable<Model.UserTable>

@{
    ViewBag.Title = "ShowUser";
      Layout = "~/Views/Shared/_AdminLayout.cshtml";

}
<link href="~/Content/AllTable.css" rel="stylesheet" />



<div class="table-wrapper">
    <table class="fl-table">

        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Id)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.UserName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Email)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Password)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Roletype)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Dept_id)
                </th>
                <th>Action</th>
            </tr>
        </thead>

        <tbody>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Id)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.UserName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Email)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Password)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Roletype)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Dept_id)
                    </td>
                    <td class="designedit">
                        @Html.ActionLink("Edit", "Edit", new { id = item.Id }, new { id = "tiid" })

                        @Html.ActionLink("Delete", "Delete", new { id = item.Id }, new { id = "deletebutton" })
                    </td>
                </tr>
            }

        </tbody>

    </table>
</div>
